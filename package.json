{
  "name": "now-secrets",
  "version": "1.0.3",
  "author": "dotPlants HQ <oss@dotplants.net>",
  "contributors": [
    "nzws <nzws@hq.dotplants.net>"
  ],
  "license": "MIT",
  "repository": "https://github.com/dotplants/now-secrets.git",
  "homepage": "https://github.com/dotplants/now-secrets",
  "engines": {
    "node": "12.x || 13.x"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint:js": "eslint --cache --ignore-path .gitignore \"**/*.js\"",
    "lint:prettier": "prettier --check --ignore-path .gitignore .",
    "lint": "npm-run-all lint:* -s",
    "format:js": "eslint --cache --fix --ignore-path .gitignore \"**/*.js\"",
    "format:prettier": "pretty-quick",
    "format": "npm-run-all format:* -s",
    "build": "babel src -d build",
    "start": "yarn build && node build",
    "prepublish": "yarn build"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@nzws/prettier-config": "^1.0.0",
    "@yuzulabo/eslint-config": "^1.0.1",
    "babel-eslint": "^10.1.0",
    "eslint": "~7",
    "npm-run-all": "~4",
    "prettier": "~2.0.5",
    "pretty-quick": "^2.0.1"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "chalk": "^3.0.0",
    "cli-ux": "^5.4.6",
    "dotenv": "^8.2.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "package.json",
    "README.md",
    "/build"
  ],
  "bin": {
    "now-secrets": "./bin/now-secrets"
  },
  "prettier": "@nzws/prettier-config"
}
